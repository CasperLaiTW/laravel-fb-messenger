{
    "name": "casperlaitw/laravel-fb-messenger",
    "type": "library",
    "description": "A Laravel Package to Integrate Facebook Messenger API",
    "keywords": [
        "facebook",
        "messenger",
        "chatbot",
        "laravel"
    ],
    "homepage": "https://github.com/casperlaitw/laravel-fb-messenger",
    "license": "MIT",
    "authors": [
        {
            "name": "Casper Lai",
            "email": "casper.lai@sleepingdesign.com",
            "role": "Developer"
        }
    ],
    "require": {
        "illuminate/support": "~5.1",
        "illuminate/routing": "~5.1",
        "illuminate/console": "~5.1",
        "php": ">=5.6.4",
        "guzzlehttp/guzzle": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.0",
        "nesbot/carbon": "^1.21",
        "satooshi/php-coveralls": "^1.0",
        "mockery/mockery": "^0.9.5",
        "fzaninotto/faker": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Casperlaitw\\LaravelFbMessenger\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/"]
    },
    "config": {
      "preferred-install": "dist"
    }
}
